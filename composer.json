{
  "name": "andrebian/brazilian-helpers",
  "description": "A set of common helpers for brazilian things",
  "type": "helper",
  "keywords": [
    "helper",
    "brazilian helpers"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Andre Cardoso",
      "email": "andrecardosodev@gmail.com",
      "role": "Author"
    }
  ],
  "homepage": "https://github.com/andrebian/brazilian-states-helper",
  "minimum-stability": "dev",
  "autoload": {
    "psr-0": {
      "BrazilianHelper\\": "src/"
    },
    "psr-4": {
      "Andrebian\\BrazilianHelpers\\": "src/"
    }
  },
  "require": {
    "php": ">5.3.2",
    "zendframework/zend-filter": "@stable"
  },
  "scripts": {
    "post-install-cmd": [
      "echo [WARNING] Delete andrebian .git dirs",
      "rm -rf vendor/andrebian/brazilian-helpers/.git"
    ],
    "post-update-cmd": [
      "echo [WARNING] Delete andrebian .git dirs",
      "rm -rf vendor/andrebian/brazilian-helpers/.git"
    ]
  }
}
